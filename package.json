{
  "name": "react-shelf",
  "version": "1.0.0",
  "description": "Front-end for pianoshelf.com made using ReactJS",
  "main": "app/main.jsx",
  "scripts": {
    "compile": "./node_modules/.bin/webpack --verbose --colors --display-error-details --config webpack.client.js",
    "lint": "./node_modules/.bin/eslint app",
    "start": "NODE_PATH=\"./app\" node babel.server",
    "watch-client": "./node_modules/.bin/webpack --quiet --display-error-details --config webpack.dev.client.js && ./node_modules/.bin/webpack-dev-server --config webpack.dev.client.js",
    "watch": "./node_modules/.bin/concurrent --kill-others \"npm run -q watch-client\" \"npm run -q start\"",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pianoshelf/react-shelf"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pianoshelf/react-shelf/issues"
  },
  "homepage": "https://github.com/pianoshelf/react-shelf",
  "devDependencies": {
    "babel-eslint": "^3.1.10",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.3",
    "concurrently": "^0.1.1",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.4.0",
    "jest-cli": "^0.4.5",
    "react-hot-loader": "^1.2.7",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "babel": "^5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "bluebird": "^2.9.27",
    "body-parser": "^1.12.4",
    "cookie-dough": "0.0.6",
    "cookie-parser": "^1.3.5",
    "express": "^4.12.4",
    "express-promise": "^0.4.0",
    "flummox": "^3.5.2",
    "lodash": "^3.9.3",
    "piping": "^0.1.8",
    "react": "^0.13.3",
    "react-a11y": "^0.1.1",
    "react-addons": "^0.9.1-deprecated",
    "react-router": "^0.13.3",
    "react-tools": "^0.13.3",
    "react-transmit": "^2.6.3",
    "superagent": "^1.2.0",
    "thenify": "^3.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ],
    "testFileExtensions": [
      "js",
      "es6",
      "jsx"
    ]
  }
}
